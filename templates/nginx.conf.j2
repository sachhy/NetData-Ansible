events {}
http {
    upstream backend {
        # the Netdata server
        server ec2-3-108-54-39.ap-south-1.compute.amazonaws.com:19999;
    }

    server {
        # nginx listens to this
        listen 80;
        server_name  $hostname;

        location / {
	    proxy_pass http://backend;
        }
    }
}
